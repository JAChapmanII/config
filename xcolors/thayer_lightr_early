! Xdefaults color scheme
! Thayer Williams - http://cinderwick.ca/
! vim:nu:ai:si:et:ts=4:sts=4:sw=4:ft=xdefaults:

! terminal colours
*foreground:#555555
*background:#eeeeee

!black
*color0:    #555555
*color8:    #222222
!red
*color1:    #ff005f
*color9:    #ff00af
!green
*color2:    #008700
*color10:   #00af00
!yellow
*color3:    #ff8700
*color11:   #ffaf00
!blue
*color4:    #000087
*color12:   #0000ff
!magenta
*color5:    #870087
*color13:   #af00d7
!cyan
*color6:    #0087af
*color14:   #00afd7
!white
*color7:    #afafaf
*color15:   #ffffff

! alsamixer scheme
alsamixer*background: #eeeeee
alsamixer*color1:     #ff005f
alsamixer*color9:     #ff005f
alsamixer*color2:     #afafaf
alsamixer*color10:    #555555
alsamixer*color7:     #ffffff
alsamixer*color15:    #ffffff
alsamixer*color11:    #777777
!alsamixer*color4:     #1b1d1e
alsamixer*color4:     #eeeeee
alsamixer*scrollBar:  false

